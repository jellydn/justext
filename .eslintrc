{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "extends": [
        "airbnb"
    ],
    "rules": {
        "import/no-mutable-exports": 0
    },
    "env": {
        "browser": true,
        "node": true
    }
}
